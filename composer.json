{
    "name": "trafficgate/transferer",
    "description": "PHP objects for handling file transfers.",
    "license": "MIT",
    "support": {
        "issues": "https://www.github.com/trafficgate/transferer/issues",
        "source": "https://www.github.com/trafficgate/transferer"
    },
    "authors": [
        {
            "name": "Andrew Miller",
            "email": "andrew.miller@rakuten.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "illuminate/console": "5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.4.*|5.5.*|5.6.*",
        "trafficgate/shell-command": "~2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.0",
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "~3.0",
        "symfony/process": "~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
            "Trafficgate\\Transferer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trafficgate\\Transferer\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Trafficgate\\Transferer\\Providers\\TransfererServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
