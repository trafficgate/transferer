{
    "name": "trafficgate/transferer",
    "description": "PHP objects for handling file transfers.",
    "license": "MIT",
    "support": {
        "issues": "https://www.github.com/trafficgate/transferer/issues",
        "source": "https://www.github.com/trafficgate/transferer"
    },
    "authors": [
        {
            "name": "Andrew Miller",
            "email": "andrew.miller@rakuten.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/console": "~5.0.0|~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0",
        "illuminate/support": "~5.0.0|~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0",
        "symfony/process": "~2.8|~3.0",
        "trafficgate/shell-command": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11",
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Trafficgate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trafficgate\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1.x-dev"
        },
        "laravel": {
            "providers": [
                "Trafficgate\\Providers\\TransfererServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
